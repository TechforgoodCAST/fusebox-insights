<div class="row pb-3">
  <div class="col-6">
    <h1>Unknowns</h1>
  </div>
  <div class="col-6 text-right">
    <%= link_to 'New Unknown', new_unknown_path, class: 'btn btn-primary' %>
  </div>
</div>

<div class="flow flow20">
  <% @unknowns.each do |unknown| %>
    <%= render partial: 'card', locals: { edit: false, unknown: unknown } %>
  <% end %>
</div>

<%= paginate @unknowns %>
